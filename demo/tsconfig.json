{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext", "webworker"],
    "types": ["vite/client"],
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "backend-ts": [".dfx/local/canisters/feed-app/feed-app.did.d.ts"],
      "backend-idl": [".dfx/local/canisters/feed-app/feed-app.did.js"]
    }
  }
}
